@model IEnumerable<EvaCatEvaluacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<!-- Ventana principal para ver los detalles-->

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DescEvaluacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DescEvaluacion)
            </td>
            <td>|
                <a asp-action="Details" asp-route-id="@item.CodEvaluacion">Detalles</a> |
            </td>
        </tr>
}
    </tbody>
</table>
